    <!--<script type="text/javascript" src="~/Content/Template/plugins/sky-forms/version-2.0.1/js/jquery-ui.min.js"></script>-->
    <script type="text/javascript" src="~/Content/Template/plugins/sky-forms/version-2.0.1/js/jquery.validate.min.js"></script>

